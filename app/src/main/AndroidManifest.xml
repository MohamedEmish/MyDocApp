<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yackeenSolution.mydocapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/app_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SearchTabActivites.DoctorDetailsActivity"></activity>
        <activity android:name=".SearchTabActivites.FacilityDetailsActivity" />
        <activity android:name=".SearchTabActivites.SearchFilterFacility" />
        <activity android:name=".SearchTabActivites.SearchResultsFacilityActivity" />
        <activity android:name=".SearchTabActivites.ConfirmationActivity" />
        <activity android:name=".SearchTabActivites.AppointmentRequestActivity" />
        <activity android:name=".SearchTabActivites.SearchFilterDoctor" />
        <activity android:name=".SearchTabActivites.SearchResultDoctorActivity" />
        <activity
            android:name=".MoreTabActivities.SettingsActivity"
            android:parentActivityName=".MainScreen"
            android:theme="@style/RegTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainScreen" />
        </activity>
        <activity
            android:name=".MoreTabActivities.ContactUsActivity"
            android:parentActivityName=".MainScreen"
            android:theme="@style/RegTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainScreen" />
        </activity>
        <activity
            android:name=".MoreTabActivities.AboutUsActivity"
            android:parentActivityName=".MainScreen"
            android:theme="@style/RegTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainScreen" />
        </activity>
        <activity
            android:name=".MoreTabActivities.PrivacyPolicyActivity"
            android:parentActivityName=".MainScreen"
            android:theme="@style/RegTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainScreen" />
        </activity>
        <activity
            android:name=".MoreTabActivities.NotificationActivity"
            android:parentActivityName=".MainScreen"
            android:theme="@style/RegTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainScreen" />
        </activity>
        <activity
            android:name=".MoreTabActivities.AddNewFamilyMember"
            android:parentActivityName=".MoreTabActivities.FamilyMembersViewer"
            android:theme="@style/RegTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MoreTabActivities.FamilyMembersViewer" />
        </activity>
        <activity
            android:name=".MoreTabActivities.FamilyMembersViewer"
            android:parentActivityName=".MainScreen"
            android:theme="@style/RegTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainScreen" />
        </activity>
        <activity android:name=".MoreTabActivities.MyAccount" />
        <activity
            android:name=".MainScreen"
            android:label="@string/title_activity_main_screen"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".ForgetPasswordActivity"
            android:label="@string/forgot_password"
            android:parentActivityName=".SignInActivity"
            android:theme="@style/RegTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".SignInActivity" />
        </activity>
        <activity
            android:name=".RegistrationActivity"
            android:label="@string/registration"
            android:theme="@style/RegTheme" />

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name"
            android:value="@string/facebook_app_id" />

        <activity android:name=".SignInActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>